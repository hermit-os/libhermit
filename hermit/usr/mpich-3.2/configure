        as_fn_error $? "Network module ${net} is unknown \"$srcdir/src/mpid/ch3/channels/nemesis/netmod/${net}\"" "$LINENO" 5
    fi
    net_macro=`echo $net | tr 'abcdefghijklmnopqrstuvwxyz' 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'`
