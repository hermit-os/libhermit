            { $as_echo "$as_me:${as_lineno-$LINENO}: checking if the F77 compiler accepts -Wl,-commons,use_dylibs" >&5
$as_echo_n "checking if the F77 compiler accepts -Wl,-commons,use_dylibs... " >&6; }
            ac_ext=f
