	WRAPPER_FCFLAGS=$FCFLAGS
	export WRAPPER_FCFLAGS



	WRAPPER_LDFLAGS=$LDFLAGS
	export WRAPPER_LDFLAGS



	WRAPPER_LIBS=$LIBS
	export WRAPPER_LIBS



	WRAPPER_EXTRA_LIBS=$EXTRA_LIBS
	export WRAPPER_EXTRA_LIBS




